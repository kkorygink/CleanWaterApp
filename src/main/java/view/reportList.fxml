<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="546.0" prefWidth="637.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.WaterReportListController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" text="Report List">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <ScrollPane fitToHeight="true" fitToWidth="true">
         <content>
            <TableView fx:id="waterReportTable">
              <columns>
                <TableColumn fx:id="reportNumberCol" prefWidth="90.0" text="no." />
                <TableColumn fx:id="userCol" prefWidth="90.0" text="User" />
                  <TableColumn fx:id="locCol" prefWidth="90.0" text="Location" />
                  <TableColumn fx:id="typeCol" prefWidth="90.0" text="Type" />
                  <TableColumn fx:id="condCol" prefWidth="90.0" text="Condition" />
                  <TableColumn fx:id="dateCol" prefWidth="90.0" text="Date" />
                  <TableColumn fx:id="timeCol" prefWidth="90.0" text="Time" />
              </columns>
            </TableView>
         </content>
      </ScrollPane>
      <Button fx:id="returnButton" mnemonicParsing="false" text="Return">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
